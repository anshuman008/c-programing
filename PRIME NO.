#include<iostream>
using namespace std;

int main(){
    
    int n;
    int a=0;
    
    cout<<"enter the number you wantr to check"<<endl;
    cin>>n;
    
    for(int i = 2 ; i<n ; i++){
        
        if(n % i==0){
          a++;
        }
    }
    
  if(a==0){
          cout<<""<<n<<" is a prime number"<<endl;
  }
    else{
         cout<<""<<n<<" is not a prime number"<<endl;
    }
    
    
    return 0;
}
