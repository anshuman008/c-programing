#include<iostream>
using namespace std;

int main(){
    
    int num;
    cout<<"enter the number which table you want to print"<<endl;
    cin>>num;
    cout<<endl;
    
    
    for(int i=1; i<=num;i++){
          cout<<endl;
    
        cout<<"***********************************************************"<<endl;
      cout<<"table of "<<i<<" is: "<<endl;
        cout<<endl;
   for(int j=1; j<=10; j++){
       
       cout<<i<<"*"<<j<<"="<<i*j<<endl;
       
       
   }
        
        
    }
    
    
    
    return 0;
}
